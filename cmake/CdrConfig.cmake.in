@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include("${CMAKE_CURRENT_LIST_DIR}/CdrTargets.cmake")

# Check each requested component
foreach(_comp ${Cdr_FIND_COMPONENTS})
  if(TARGET cdr::${_comp})
    set(Cdr_${_comp}_FOUND TRUE)
  else()
    set(Cdr_${_comp}_FOUND FALSE)
  endif()
endforeach()

check_required_components(Cdr)
